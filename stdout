#!/bin/wpemu -r -f
                *
                * stdout.s	put some ascii on the screen
                *

000000 41000001	write	MOV	$1,R1
000004 a1e00000		MOV	R1,0(R14)
000008 41000028		MOV	$data,R1
00000c a1e00004		MOV	R1,4(R14)
000010 41000020		MOV	$len,R1
000014 a1e00008		MOV	R1,8(R14)
000018 40000002		MOV	$2,R0
00001c c7000010		SYSCALL
000020 40000004	end	MOV	$4,R0
000024 c7000010		SYSCALL

000028 454d4f53	data	STRINGZ	'SOME ASCII OUTPUT FOR YOU\n'
00002c 43534120	
000030 4f204949	
000034 55505455	
000038 4f462054	
00003c 4f592052	
000040 00000a55	
       00000001	out	EQU	1
       00000020	len	EQU	32

                Symbol Table
S 0x000000 write
S 0x000020 end
S 0x000028 data
S 0x000001 out
S 0x000020 len
